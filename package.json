{
  "name": "xando",
  "version": "1.0.0",
  "description": "tic tac toe App",
  "main": "app.js",
  "repository": "https://github.com/dev-jsx/toenail-api",
  "author": "midlajc | ali",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc && tsc-alias",
    "prisma:init": "dotenv -e .env -- prisma init",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:studio": "dotenv -e .env -- prisma studio"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "socket.io": "^4.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.18",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3"
  }
}
